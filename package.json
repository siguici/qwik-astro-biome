{
  "name": "qwik-astro-biome",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "build": "pnpm fix && astro build",
    "check": "biome ci . && astro check",
    "check.format": "biome format .",
    "check.lint": "biome check .",
    "dev": "astro dev",
    "fix": "pnpm lint && pnpm format && pnpm sync",
    "format": "biome format --write .",
    "lint": "biome check --apply-unsafe .",
    "preview": "pnpm build && pnpm serve",
    "prod": "pnpm build && pnpm deploy",
    "serve": "node ./dist/server/entry.mjs",
    "start": "astro dev --open"
  },
  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/node": "^8.2.5",
    "@builder.io/qwik": "^1.5.2",
    "@qwikdev/astro": "^0.5.14",
    "astro": "^4.6.3",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.0"
  }
}
